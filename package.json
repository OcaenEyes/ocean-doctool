{
  "name": "ocean-doctool",
  "private": true,
  "version": "0.0.1",
  "author": "OceanGZY",
  "copyright": "Copyright Â© 2021 OceanGZY",
  "license": "MIT",
  "description": "A tool for documenting Vue3 components.",
  "main": "./dist/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:dev": "chcp 65001 && vite --config vite.config.electron.ts",
    "electron:build": "vue-tsc && vite build --config vite.config.electron.ts && electron-builder"
  },
  "dependencies": {
    "@antv/x6": "^2.15.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.8.5",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.ocean.doctool",
    "productName": "OceanDoctool",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    }
  }
}